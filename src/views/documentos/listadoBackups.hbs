<!-- CUERPO -->
<div class="container-fluid">
    <!-- id="cuerpo" -->
     <div class="text-center m-4"> <a class="btn btn-primary" href="/dumpSQL">Crear dumpSQL</a></div>
    <h1 class="text-center">LISTADO BACKUPS</h1>
    <div class="row my-4">
        <div class="col-11 mx-auto">
            <table class="table table-bordered table-dark table-hover my-4">
                <tr>
                    <th>#</th>
                    <th>Nombre</th>
                    <th>Creado</th>
                    <th>acciones</th>
                </tr>
                {{#if backups}}
                {{#each backups}}
                <tr>
                    <td> {{@key}}</td>
                    <td>
                        <a href="/dumpSQL/{{this.name}}" target="_blank"> {{this.name}} </a>
                    </td>
               
                    <td>{{ timeago this.created_at}}</td>
                    <td> <a type="submit" href="/backups/del/{{this.name}}" class="text-primary"><i
                                class="fa-solid fa-trash"></i>Del</a>
                    </td>
                </tr>
                {{/each}}
                {{else}}
                <tr>No hay ning√∫n backup generado, <a href="/dumpSQL">crea uno</a> !! </tr>
                {{/if}}
            </table>
        </div>
    </div>
</div>